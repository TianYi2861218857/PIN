<!--miniprogram/pages/editUserInfo/editUserInfo.wxml-->

<view class="editUserInfo">
  <navigator url="./show/show" open-type="navigate">
    <view class="editUserInfo-list-item">
      <text>展示图设置</text>
      <text class="iconfont icon-youjiantou"></text>
    </view>
  </navigator>
  <navigator url="./name/name" open-type="navigate">
    <view class="editUserInfo-list-item">
      <text>名称设置</text>
      <text class="iconfont icon-youjiantou"></text>
    </view>
  </navigator>
  <navigator url="./explain/explain" open-type="navigate">
    <view class="editUserInfo-list-item">
      <text>信息说明设置</text>
      <text class="iconfont icon-youjiantou"></text>
    </view>
  </navigator>
  <navigator url="./publish/publish" open-type="navigate">
    <view class="editUserInfo-list-item">
      <text>发布设置</text>
      <text class="iconfont icon-youjiantou"></text>
    </view>
  </navigator>
  <navigator url="./phone/phone" open-type="navigate">
    <view class="editUserInfo-list-item">
      <text>手机号<text class="editUserInfo-list-tips">参与"拼一下"可见</text></text>
      <text class="iconfont icon-youjiantou"></text>
    </view>
  </navigator>
  <navigator url="./weixin/weixin" open-type="navigate">
    <view class="editUserInfo-list-item">
      <text>微信号<text class="editUserInfo-list-tips">参与"拼一下"可见</text></text>
      <text class="iconfont icon-youjiantou"></text>
    </view>
  </navigator>
</view>
